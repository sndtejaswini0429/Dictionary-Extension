{
    "name" : " Dictionary extension",
    "version" : "0.1",
    "manifest_version" : 2,
    "description" : "this is a extension which helps you to find the meaning of a word",
    "permissions": ["activeTab"],
    "icons":{
        "16":"icon.jpg"
        },
    "browser_action":{
        "default_popup":"index.html"
    },
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js": ["index.js"]        }
    ]
}
